@model IEnumerable<BugTracker.Models.TicketHistory>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ticket.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProjectId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OldProjectId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TicketTypeId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OldTicketTypeId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TicketPriorityId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OldTicketPriorityId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TicketStatusId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OldTicketStatusId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OwnerUserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssignedToUserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OldAssignedToUserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HideTicket)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OldHideTicket)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Changed)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Ticket.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProjectId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OldProjectId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TicketTypeId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OldTicketTypeId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TicketPriorityId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OldTicketPriorityId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TicketStatusId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OldTicketStatusId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OwnerUserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AssignedToUserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OldAssignedToUserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HideTicket)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OldHideTicket)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Changed)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
